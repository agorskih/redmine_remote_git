<%= form_for repository, url: {action: "create"} do |f| %>
	<table>
		<tr>
			<td> <%= label_tag :remote_origin_url, 'repository remote origin url:' %> </td>
			<td> <%= f.text_field :remote_origin_url %> </td>
		</tr>
		<tr>
			<td> <%= label_tag :local_clone_path, 'repository local clone path:' %> </td>
			<td> <%= f.text_field :local_clone_path %> </td>
		</tr>
		<tr>
			<td>  </td>
			<td align="right"> <%= f.submit "Create" %> </td>
		</tr>
	</table>
<% end %>